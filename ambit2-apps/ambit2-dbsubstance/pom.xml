<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>ambit2-apps</artifactId>
    <groupId>ambit</groupId>
    <version>2.7.1-SNAPSHOT</version>
  </parent>
  <groupId>ambit</groupId>
  <artifactId>ambit2-dbsubstance</artifactId>
  <version>2.7.1-SNAPSHOT</version>
  <name>ambit2-dbsubstance</name>
  <description>Substance import with formats different than I5Z</description>
<dependencies>
    <dependency>
      <groupId>org.dbunit</groupId>
      <artifactId>dbunit</artifactId>
      <version>${dbunit.version}</version>
      <scope>test</scope>
    </dependency>

   <dependency>
      <groupId>ambit</groupId>
      <artifactId>ambit2-db</artifactId>
      <version>${project.version}</version>
      <type>jar</type>
      <scope>compile</scope>
    </dependency>
   <dependency>
      <groupId>ambit</groupId>
      <artifactId>ambit2-db</artifactId>
      <version>${project.version}</version>
      <classifier>tests</classifier>
      <type>jar</type>
      <scope>test</scope>
    </dependency>    
   <dependency>
      <groupId>ambit</groupId>
      <artifactId>ambit2-dbi5</artifactId>
      <version>${project.version}</version>
      <type>jar</type>
    </dependency>
    <dependency>
    	<groupId>net.ideaconsult</groupId>
    	<artifactId>loom-nm</artifactId>
    	<version>${net.idea.loom.version}</version>
    	<type>jar</type>
    </dependency>      
    <dependency>
    	<groupId>net.ideaconsult</groupId>
    	<artifactId>loom-tox21</artifactId>
    	<version>${net.idea.loom.version}</version>
    	<type>jar</type>
    </dependency>
    <dependency>
    	<groupId>net.ideaconsult</groupId>
    	<artifactId>loom-pubchemrest-cli</artifactId>
    	<version>${net.idea.loom.version}</version>
    	<type>jar</type>
    </dependency>
            
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>${junit.version}</version>
      <scope>test</scope>
    </dependency>    
</dependencies>  
  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-surefire-plugin</artifactId>
          <version>2.6</version>
          <configuration>
            <skip>true</skip>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
</project>