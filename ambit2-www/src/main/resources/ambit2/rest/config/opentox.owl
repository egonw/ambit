        <dc:title rdf:datatype="&xsd;string"
            >NumFalsePositives</dc:title>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#numFolds -->

    <owl:DatatypeProperty rdf:about="#numFolds">
        <rdfs:domain rdf:resource="#Crossvalidation"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#numTruePositives -->

    <owl:DatatypeProperty rdf:about="#numTruePositives">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:title rdf:datatype="&xsd;string"
            >NumTruePositives</dc:title>
        <rdfs:comment rdf:datatype="&xsd;string"
            >NumTruePositives</rdfs:comment>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#numTrue_negatives -->

    <owl:DatatypeProperty rdf:about="#numTrue_negatives">
