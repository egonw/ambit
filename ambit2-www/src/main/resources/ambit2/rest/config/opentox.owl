    <!-- http://www.opentox.org/api/1.1#numberCorrect -->

    <owl:DatatypeProperty rdf:about="#numberCorrect">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:title rdf:datatype="&xsd;string">Number correct</dc:title>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
