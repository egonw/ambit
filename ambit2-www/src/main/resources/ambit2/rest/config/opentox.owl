        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#numFalsePositives -->

    <owl:DatatypeProperty rdf:about="#numFalsePositives">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
