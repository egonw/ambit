<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY ot "http://www.opentox.org/api/1.1#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
]>


<rdf:RDF xmlns="http://www.opentox.org/api/1.1#"
     xml:base="http://www.opentox.org/api/1.1"
     xmlns:ot="http://www.opentox.org/api/1.1#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="">
        <dc:date rdf:datatype="&xsd;string">2009-11-22</dc:date>
        <dc:creator rdf:datatype="&xsd;string">OpenTox</dc:creator>
        <dc:title rdf:datatype="&xsd;string">OpenTox API</dc:title>
        <rdfs:comment rdf:datatype="&xsd;string"
            >OpenTox resource ontology</rdfs:comment>
        <dc:identifier rdf:datatype="&xsd;string"
            >http://opentox.org/dev/apis/api-1.1</dc:identifier>
        <dc:contributor rdf:datatype="&xsd;string"
            >jeliazkova.nina@gmail.com</dc:contributor>
        <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;format"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    <owl:AnnotationProperty rdf:about="&dc;source"/>
    <owl:AnnotationProperty rdf:about="&dc;identifier"/>
    <owl:AnnotationProperty rdf:about="&dc;publisher"/>
    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.opentox.org/api/1.1#algorithm -->

    <owl:ObjectProperty rdf:about="#algorithm">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The algorithm, used to create the Model</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#Algorithm"/>
        <rdfs:domain rdf:resource="#Model"/>
        <rdfs:subPropertyOf rdf:resource="#model"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#compound -->

    <owl:ObjectProperty rdf:about="#compound">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A DataEntry is defined with a single compound and multiple feature values. This property sets the relationship between a DataEntry and a Compound</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#Compound"/>
        <rdfs:domain rdf:resource="#DataEntry"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#conformer -->

    <owl:ObjectProperty rdf:about="#conformer">
        <rdfs:domain rdf:resource="#Compound"/>
        <rdfs:range rdf:resource="#Conformer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#crossvalidationAlgorithm -->

    <owl:ObjectProperty rdf:about="#crossvalidationAlgorithm">
        <rdfs:range rdf:resource="#Algorithm"/>
        <rdfs:domain rdf:resource="#Crossvalidation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#crossvalidationDataset -->

    <owl:ObjectProperty rdf:about="#crossvalidationDataset">
        <rdfs:domain rdf:resource="#Crossvalidation"/>
        <rdfs:range rdf:resource="#Dataset"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#crossvalidationPredictedVariable -->

    <owl:ObjectProperty rdf:about="#crossvalidationPredictedVariable">
        <rdfs:domain rdf:resource="#Crossvalidation"/>
        <rdfs:range rdf:resource="#Feature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#dataEntry -->

    <owl:ObjectProperty rdf:about="#dataEntry">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A Dataset contains multiple DataEntries.  This property specifies the relationship between Dataset and DataEntry.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#DataEntry"/>
        <rdfs:domain rdf:resource="#Dataset"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#dependentVariables -->

    <owl:ObjectProperty rdf:about="#dependentVariables">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A model can have one or more dependent variables, described as multiple features, specified by this relationship.</rdfs:comment>
        <rdfs:range rdf:resource="#Feature"/>
        <rdfs:domain rdf:resource="#Model"/>
        <rdfs:subPropertyOf rdf:resource="#model"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#feature -->

    <owl:ObjectProperty rdf:about="#feature">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >FeatureValue contains a value for specific Feature, specified by this relationship.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#Feature"/>
        <rdfs:domain rdf:resource="#FeatureValue"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#hasSource -->

    <owl:ObjectProperty rdf:about="#hasSource">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Feature"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Algorithm"/>
                    <rdf:Description rdf:about="#Dataset"/>
                    <rdf:Description rdf:about="#Model"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#hasValidationInfo -->

    <owl:ObjectProperty rdf:about="#hasValidationInfo">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Links Validation with Validation Info. One validation exercise may have multiple types of validation informaton</rdfs:comment>
        <rdfs:domain rdf:resource="#Validation"/>
        <rdfs:range rdf:resource="#ValidationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#validation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#independentVariables -->

    <owl:ObjectProperty rdf:about="#independentVariables">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A model can have multiple independent variables, described as multiple features, specified by this relationship.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#Feature"/>
        <rdfs:domain rdf:resource="#Model"/>
        <rdfs:subPropertyOf rdf:resource="#model"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#isA -->

    <owl:ObjectProperty rdf:about="#isA">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Allows to define &quot;is a&quot; relationships outside of particular class hierarchy</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#model -->

    <owl:ObjectProperty rdf:about="#model">
        <rdfs:range rdf:resource="#Dataset"/>
        <rdfs:domain rdf:resource="#Model"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#parameters -->

    <owl:ObjectProperty rdf:about="#parameters">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Algorithms and Models can have multiple parameters</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#Parameter"/>
        <rdfs:subPropertyOf rdf:resource="#model"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Algorithm"/>
                    <rdf:Description rdf:about="#Model"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#predictedVariables -->

    <owl:ObjectProperty rdf:about="#predictedVariables">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Variables, holding the predicted values, generated by the model</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#Feature"/>
        <rdfs:domain rdf:resource="#Model"/>
        <rdfs:subPropertyOf rdf:resource="#model"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#report -->

    <owl:ObjectProperty rdf:about="#report">
        <rdfs:domain rdf:resource="#Report"/>
        <rdfs:range rdf:resource="#Validation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#trainingDataset -->

    <owl:ObjectProperty rdf:about="#trainingDataset">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A model is derived by applying an Algorithm on a training Dataset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="#Dataset"/>
        <rdfs:domain rdf:resource="#Model"/>
        <rdfs:subPropertyOf rdf:resource="#model"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#unscrambledDatasetURI -->

    <owl:ObjectProperty rdf:about="#unscrambledDatasetURI">
        <rdfs:comment rdf:datatype="&xsd;string"
            >UnscrambledDatasetURI</rdfs:comment>
        <dc:title rdf:datatype="&xsd;string"
            >a link to UnscrambledDataset</dc:title>
        <rdfs:range rdf:resource="#Dataset"/>
        <rdfs:domain rdf:resource="#YScrambling"/>
        <rdfs:subPropertyOf rdf:resource="#validation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#validation -->

    <owl:ObjectProperty rdf:about="#validation">
        <rdfs:domain rdf:resource="#Validation"/>
        <rdfs:range rdf:resource="#ValidationInfo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#validationCrossvalidation -->

    <owl:ObjectProperty rdf:about="#validationCrossvalidation">
        <rdfs:domain rdf:resource="#CrossValidationInfo"/>
        <rdfs:range rdf:resource="#Crossvalidation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#validationModel -->

    <owl:ObjectProperty rdf:about="#validationModel">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Model used in a validation exercise</rdfs:comment>
        <rdfs:range rdf:resource="#Model"/>
        <rdfs:domain rdf:resource="#Validation"/>
        <rdfs:subPropertyOf rdf:resource="#validation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#validationPredictionDataset -->

    <owl:ObjectProperty rdf:about="#validationPredictionDataset">
        <rdfs:comment rdf:datatype="&xsd;string"
            >PredictionDatasetURI</rdfs:comment>
        <rdfs:range rdf:resource="#Dataset"/>
        <rdfs:domain rdf:resource="#Validation"/>
        <rdfs:subPropertyOf rdf:resource="#validation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#validationReport -->

    <owl:ObjectProperty rdf:about="#validationReport">
        <rdfs:domain rdf:resource="#Report"/>
        <rdfs:range rdf:resource="#Validation"/>
        <rdfs:subPropertyOf rdf:resource="#report"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#validationTestDataset -->

    <owl:ObjectProperty rdf:about="#validationTestDataset">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Test dataset , used in a validation exercise</rdfs:comment>
        <rdfs:range rdf:resource="#Dataset"/>
        <rdfs:domain rdf:resource="#Validation"/>
        <rdfs:subPropertyOf rdf:resource="#validation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#validationTrainingDataset -->

    <owl:ObjectProperty rdf:about="#validationTrainingDataset">
        <rdfs:range rdf:resource="#Dataset"/>
        <rdfs:domain rdf:resource="#Validation"/>
        <rdfs:subPropertyOf rdf:resource="#validation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opentox.org/api/1.1#values -->

    <owl:ObjectProperty rdf:about="#values">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A DataEntry is defined with a single compound and multiple feature values. This property sets the relationship between a DataEntry and multiple FeatureValues</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string"
            >http://opentox.org/api/1.1</rdfs:isDefinedBy>
        <rdfs:domain rdf:resource="#DataEntry"/>
        <rdfs:range rdf:resource="#FeatureValue"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.opentox.org/api/1.1#CPURuntime -->

    <owl:DatatypeProperty rdf:about="#CPURuntime">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Crossvalidation"/>
        <rdfs:domain rdf:resource="#Validation"/>
        <rdfs:range rdf:resource="&xsd;long"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#areaUnderROC -->

    <owl:DatatypeProperty rdf:about="#areaUnderROC">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">AreaUnderROC</rdfs:comment>
        <dc:title rdf:datatype="&xsd;string">AreaUnderROC</dc:title>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#classificationInfo -->

    <owl:DatatypeProperty rdf:about="#classificationInfo">
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#crossValidationInfo -->

    <owl:DatatypeProperty rdf:about="#crossValidationInfo">
        <rdfs:domain rdf:resource="#CrossValidationInfo"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#fMeasure -->

    <owl:DatatypeProperty rdf:about="#fMeasure">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:title rdf:datatype="&xsd;string">FMeasure</dc:title>
        <rdfs:comment rdf:datatype="&xsd;string">FMeasure</rdfs:comment>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#falseNegativeRate -->

    <owl:DatatypeProperty rdf:about="#falseNegativeRate">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:title rdf:datatype="&xsd;string"
            >FalseNegativeRate</dc:title>
        <rdfs:comment rdf:datatype="&xsd;string"
            >FalseNegativeRate</rdfs:comment>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#falsePositiveRate -->

    <owl:DatatypeProperty rdf:about="#falsePositiveRate">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:identifier rdf:datatype="&xsd;string"
            >FalsePositiveRate</dc:identifier>
        <rdfs:comment rdf:datatype="&xsd;string"
            >FalsePositiveRate</rdfs:comment>
        <dc:title rdf:datatype="&xsd;string"
            >FalsePositiveRate</dc:title>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#fold -->

    <owl:DatatypeProperty rdf:about="#fold">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#CrossValidationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#crossValidationInfo"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#has3Dstructure -->

    <owl:DatatypeProperty rdf:about="#has3Dstructure">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:title rdf:datatype="&xsd;string"
            >Has 3D structure</dc:title>
        <rdfs:comment rdf:datatype="&xsd;string"
            >True, if the compound has 3D structure</rdfs:comment>
        <rdfs:domain rdf:resource="#Compound"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#hasStatus -->

    <owl:DatatypeProperty rdf:about="#hasStatus">
        <rdfs:domain rdf:resource="#Task"/>
        <rdfs:range>
            <rdf:Description>
                <rdf:type rdf:resource="&owl;DataRange"/>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">Cancelled</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">Completed</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">Running</rdf:first>
                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdf:Description>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#kappaStatistic -->

    <owl:DatatypeProperty rdf:about="#kappaStatistic">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:title rdf:datatype="&xsd;string"
            >Kappa Statistics</dc:title>
        <rdfs:domain rdf:resource="#ClassificationInfo"/>
        <rdfs:subPropertyOf rdf:resource="#classificationInfo"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.opentox.org/api/1.1#meanAbsolutError -->

    <owl:DatatypeProperty rdf:about="#meanAbsolutError">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >MeanAbsolutError</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#regressionInfo"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


