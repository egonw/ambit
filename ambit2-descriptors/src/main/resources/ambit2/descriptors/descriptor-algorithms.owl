        </description>
        <isClassifiedAs rdf:resource="&me;topologicalDescriptor"/>
    </MolecularDescriptor>

    <MolecularDescriptor rdf:about="&me;chiCluster">
        <rdfs:label>Chi Cluster Indices</rdfs:label>
        <dc:contributor rdf:resource="&me;rguha"/>
        <dc:date>2006-11-13</dc:date>
        <definition rdf:parseType='Literal'>
            Evaluates the Kier &amp; Hall Chi cluster indices of orders 3,4,5,6 and 7
        </definition>
        <description rdf:parseType='Literal'>
