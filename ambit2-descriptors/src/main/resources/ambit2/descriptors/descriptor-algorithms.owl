        </description>
        <isClassifiedAs rdf:resource="&me;topologicalDescriptor"/>
    </MolecularDescriptor>

    <MolecularDescriptor rdf:about="&me;chiPath">
        <rdfs:label>Chi Path Indices</rdfs:label>
