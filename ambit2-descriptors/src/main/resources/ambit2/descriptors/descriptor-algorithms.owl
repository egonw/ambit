    <DataRepresentation rdf:about="&me;DietzRepresentation">
        <rdfs:label>Dietz Representation</rdfs:label>
        <owl:differentFrom rdf:resource="&me;GraphRepresentation"/>
        <!-- FIXME: add description and reference to Dietz' article in JCICS -->
    </DataRepresentation>

    <DataRepresentation rdf:about="&me;GraphRepresentation">
        <rdfs:label>Graph Representation</rdfs:label>
        <owl:differentFrom rdf:resource="&me;DietzRepresentation"/>
        <description rdf:parseType='Literal'>
            Representation in which atoms are represented by graph nodes and
            bonds by graph edges. Consequently, bonds are always localized
            between two atoms.
        </description>
    </DataRepresentation>

