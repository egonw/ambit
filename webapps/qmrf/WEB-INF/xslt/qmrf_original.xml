  <Catalogs>
    <Software_catalog>
      <Software id="ADAPT" name="ADAPT" url="http://zeus.chem.psu.edu" number="1">
      <software_description>
      A QSAR toolkit with descriptor generation (topological, geometrical, electronic, and physicochemical descriptors), 
      variable selection, regression and artificial network modelling.
      </software_description>
      <contact>Prof. P.C. Jurs, PennState University, University Park PA 16802, USA</contact>
      <references><article_ref idref="Jurs79"/></references>
      </Software>
      

      <Software id="CODESSA" name="CODESSA" url="http://semichem.com"  number="2">
      <software_description>
      Calculation of several topological, geometrical, constitutional, thermodynamic, electrostatic and quantum-chemical descriptors, including tools for regression modelling and variable selection.
      </software_description>
      <contact>
      Semichem Inc. - 7204 Mullen, Shawnee, KS 66216, USA
      </contact>
      <references><book_ref idref="Katritzky95"/></references>
      </Software>
            
      <Software id="DRAGON" name="DRAGON" url="http://www.disat.unimib.it/chm"  number="3">
      <software_description>
      Calculation of several sets of molecular descriptors from molecular geometries (topological, geometrical, WHIM, 3D-MoRSE, molecular profiles, etc.)
      </software_description>
      <contact>
      Prof. R.Todeschini - distributed by Talete srl, via Pisani 13, 20124 Milano, Italy
      </contact>
      <references/>

      </Software>
      
      <Software id="TSAR" name="TSAR" url="http://www.oxmol.com"  number="4">
      <software_description>
      Statistical and database functions with molecular and substituent property calculations. Within TSAR 3D package.
      </software_description>
      <contact>
      Oxford Molecular Ltd., Oxford Science Park, Oxford OX4 4GA, UK
      </contact>
      <references></references>
      </Software>
            
    </Software_catalog>
    
    
